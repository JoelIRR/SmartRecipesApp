{
  "ReverseProxy": {
    "Routes": {
      "recetasRoute": {
        "ClusterId": "recetasCluster",
        "Match": {
          "Path": "/recetas/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/recetas" }
        ]
      },
      "analisisRoute": {
        "ClusterId": "analisisCluster",
        "Match": {
          "Path": "/analisis/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/analisis" }
        ]
      }
    },
    "Clusters": {
      "recetasCluster": {
        "Destinations": {
          "recetasapi": {
            "Address": "http://recetasapi:8080/"
          }
        }
      },
      "analisisCluster": {
        "Destinations": {
          "analisisservice": {
            "Address": "http://analisisservice:8080/"
          }
        }
      }
    }
  }
}
